{
  "extends": ["braintree/client", "prettier"],
  "globals": {
    "Promise": "readonly",
    "process": "readonly"
  },
  "rules": {
    "valid-jsdoc": [
      2,
      {
        "prefer": {
          "return": "returns"
        },
        "requireReturn": true,
        "requireParamDescription": true,
        "requireReturnDescription": true
      }
    ]
  }
}
